{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [ "@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ],
  "sourceType": "unambiguous",
  "env": {
    "development": {
      "presets": []
    },
    "test": {
      "plugins": [
        [
          "module-resolver", {
            "root": [
                "app",
                "@isle-project"
              ]
            }
        ]
      ]
    }
  }
}
